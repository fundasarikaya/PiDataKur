
@model List<PiData.Business.Common.ExchangeListDTO>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Tarih</th>
            <th>Para Birimi</th>
            <th>Döviz Alış</th>
            <th>Döviz Satış</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Date</td>
                <td>@item.CurrencyName</td>
                <td>@item.ForexBuying</td>
                <td>@item.ForexSelling</td>
            </tr>

        }
    </tbody>
</table>


