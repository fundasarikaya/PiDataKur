@model List<PiData.Business.Common.ExchangeListDTO>

<table class="table table-hover" style="overflow-y: scroll;">
    <tr>
        <th>Para Birimi</th>
        <th>Döviz Alış</th>
        <th>Döviz Satış</th>
    </tr>
    @foreach (var item in Model)
        {
            <tr>
                <td>@item.CurrencyName</td>
                <td>@item.ForexBuying</td>
                <td>@item.ForexSelling</td>
            </tr>
        }

</table>